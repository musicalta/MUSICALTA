<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_template_product_form_inherit" model="ir.ui.view">
        <field name="name">product.template.product.form.inherit</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_only_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='uom_po_id']" position="after">
                <field name="professor_id"/>
                <field name="is_product_hebergement"/>
                <field name="is_work_rooms"/>
                <field name="is_one_day_room"/>
                <field name="is_product_launch"/>
                <field name="number_of_meal"/>
                <field name="discipline_id"/>
                <field name="is_fees"/>
                <field name="is_product_for_adults"/>
            </xpath>
        </field>
    </record>
</odoo>
