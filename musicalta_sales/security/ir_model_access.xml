<odoo>
    <record id="event_inscription_wizard_access" model="ir.model.access">
        <field name="name">wizard.event.inscription.access</field>
        <field name="model_id" ref="model_wizard_event_inscription"/>
        <field name="group_id" ref="sales_team.group_sale_salesman"/>
        <field name="perm_read" eval="True"/>
        <field name="perm_create" eval="True"/>
        <field name="perm_write" eval="True"/>
        <field name="perm_unlink" eval="False"/>
    </record>

    <record id="event_inscription_training_access" model="ir.model.access">
        <field name="name">wizard.event.inscription.training.access</field>
        <field name="model_id" ref="model_wizard_event_inscription_training"/>
        <field name="group_id" ref="sales_team.group_sale_salesman"/>
        <field name="perm_read" eval="True"/>
        <field name="perm_create" eval="True"/>
        <field name="perm_write" eval="True"/>
        <field name="perm_unlink" eval="True"/>
    </record>
    
    <record id="lunch_order_access_rule" model="ir.model.access">
        <field name="name">lunch.order.access</field>
        <field name="model_id" ref="model_event_lunch_order"/>
        <field name="perm_read">1</field>
        <field name="perm_write">1</field>
        <field name="perm_create">1</field>
        <field name="perm_unlink">1</field>
    </record>

</odoo>
